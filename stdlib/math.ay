func min(a: int, b: int) int {
	if a < b {
		return a;
	}

	return b;
}

func max(a: int, b: int) int {
	if a > b {
		return a;
	}
	
	return b;
}

func degree_to_radian(deg: f64) f64 {
    return deg * (PI / 180.0); 
}

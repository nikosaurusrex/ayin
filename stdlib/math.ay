PI :: 3.14159265359

func cos(x f64) f64 #foreign;
func acos(x f64) f64 #foreign;
func sin(x f64) f64 #foreign;
func asin(x f64) f64 #foreign;
func tan(x f64) f64 #foreign;
func atan(x f64) f64 #foreign;

func ceil(x f64) f64 #foreign;
